pipeline {
    agent any
    stages {
        stage('Stage 1') {
            steps {
                withCredentials([string(credentialsId: TANZU_API_TOKEN_API, variable: 'TANZU_API_TOKEN_API')]) {
                    sh('TANZU_API_TOKEN=$TANZU_API_TOKEN_API tanzu login')
                }
                sh 'tanzu build'
            }
        }
    }
}
